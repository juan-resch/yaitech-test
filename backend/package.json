{
  "name": "nestjs-docker-template",
  "version": "1.0.0",
  "description": "This template uses NestJS, Typeorm, Docker, Postgres, unit test integration, end-to-end and much more",
  "author": "Juan Resch",
  "private": true,
  "scripts": {
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "node create-database-if-not-exists.js && npm run typeorm:sync && node dist/src/main.js",
    "typeorm:sync": "typeorm schema:sync",
    "dev": "node create-database-if-not-exists.js && nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:cov": "vitest run --coverage",
    "test:ui": "vitest --ui",
    "test:ui:coverage": "vitest --ui --coverage.enabled=true",
    "test:debug": "vitest --inspect-brk --inspect --logHeapUsage --threads=false",
    "test:e2e": "vitest run --config ./vitest.config.e2e.mts",
    "lint:fix": "eslint --fix .",
    "husky:prepare": "husky init"
  },
  "dependencies": {
    "@faker-js/faker": "^8.4.0",
    "@google/generative-ai": "^0.17.1",
    "@huggingface/inference": "^2.8.1",
    "@langchain/community": "^0.3.22",
    "@langchain/core": "^0.3.27",
    "@langchain/google-vertexai": "^0.1.6",
    "@langchain/openai": "^0.3.16",
    "@nestjs/cli": "^9.0.0",
    "@nestjs/common": "^10.3.10",
    "@nestjs/config": "^3.1.1",
    "@nestjs/core": "^10.3.10",
    "@nestjs/event-emitter": "^2.0.4",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/platform-express": "^10.4.15",
    "@nestjs/serve-static": "^4.0.2",
    "@nestjs/swagger": "^7.2.0",
    "@nestjs/typeorm": "^10.0.2",
    "@pinecone-database/pinecone": "^4.0.0",
    "@prisma/client": "^5.11.0",
    "@sentry/node": "^7.93.0",
    "@sentry/tracing": "^7.93.0",
    "@types/moment": "^2.13.0",
    "@types/pdf-parse": "^1.1.4",
    "@vtal/eslint-config": "^1.0.6",
    "bcryptjs": "^2.4.3",
    "cohere-ai": "^7.15.2",
    "dayjs": "^1.11.10",
    "dotenv": "^16.4.1",
    "fs": "^0.0.1-security",
    "husky": "^9.0.11",
    "langchain": "^0.3.10",
    "moment": "^2.30.1",
    "multer": "^1.4.5-lts.1",
    "nodemailer": "^6.9.13",
    "openai": "^4.77.3",
    "pdf-parse": "^1.1.1",
    "pg": "^8.12.0",
    "reflect-metadata": "^0.1.13",
    "resend": "^3.2.0",
    "rxjs": "^7.2.0",
    "slugify": "^1.6.6",
    "swagger-ui-express": "^5.0.0",
    "typeorm": "^0.3.20",
    "zod": "^3.22.4",
    "zod-validation-error": "^3.0.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^18.6.0",
    "@commitlint/config-conventional": "^18.6.0",
    "@nestjs/schematics": "^9.0.0",
    "@nestjs/testing": "^10.3.10",
    "@swc/core": "^1.3.103",
    "@types/bcryptjs": "^2.4.6",
    "@types/express": "^4.17.13",
    "@types/jest": "29.5.0",
    "@types/lodash": "^4.14.202",
    "@types/multer": "^1.4.11",
    "@types/node": "18.15.11",
    "@types/nodemailer": "^6.4.15",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.0.0",
    "@typescript-eslint/parser": "^5.0.0",
    "@vitest/coverage-v8": "^1.2.0",
    "@vitest/ui": "^1.6.0",
    "eslint": "^8.0.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-prettier": "^4.0.0",
    "jest": "29.5.0",
    "prettier": "^2.3.2",
    "prisma": "^5.11.0",
    "source-map-support": "^0.5.20",
    "supertest": "^6.3.4",
    "ts-jest": "29.0.5",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.0.0",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "4.2.0",
    "typescript": "^4.7.4",
    "unplugin-swc": "^1.4.4",
    "vite-tsconfig-paths": "^4.3.0",
    "vitest": "^1.2.0"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "json",
      "ts"
    ],
    "rootDir": "src",
    "testRegex": ".*\\.spec\\.ts$",
    "transform": {
      "^.+\\.(t|j)s$": "ts-jest"
    },
    "collectCoverageFrom": [
      "**/*.(t|j)s"
    ],
    "coverageDirectory": "../coverage",
    "testEnvironment": "node"
  }
}
